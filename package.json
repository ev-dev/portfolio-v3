{
  "name": "ev-dev-starter",
  "version": "0.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build-dev & npm run serve-dev",
    "build-dev": "NODE_ENV=development webpack -w",
    "serve-dev": "NODE_ENV=development nodemon -r @std/esm src/server --watch src/server",
    "test-serve": "NODE_ENV=development nodemon -r @std/esm src/server/tests",
    "test-build": "NODE_ENV=development webpack --bail --progress -d",
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config config/dev.config.babel.js --progress",
    "build": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=production node src/server"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "ev-dev",
  "license": "MIT",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-hot-loader": "^1.3.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
